<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

        <PreferenceCategory
            app:allowDividerAbove="false"
            app:allowDividerBelow="false"
            app:iconSpaceReserved="false">

                <Preference
                    android:icon="@drawable/ic_settings"
                    android:title="lool"
                    android:summary="lox">


                </Preference>
        </PreferenceCategory>


        <PreferenceCategory android:title="@string/app_name"
            android:iconSpaceReserved="false">
                <Preference
                    app:iconSpaceReserved="false"
                    android:title="@string/ratingApp"
                    android:summary="@string/open_PlayMarket"
                    app:enabled="true"
                    app:key="ratingApp"/>
                <Preference
                    app:iconSpaceReserved="false"
                    android:summary="@string/telegram"
                    android:title="@string/sendDev"
                    app:enabled="true">
                        <intent
                            android:action="android.intent.action.VIEW"
                            android:data="https://pasichdev.github.io/pasich/contact.html" />
                </Preference>
                <Preference
                    app:iconSpaceReserved="false"
                    android:title="@string/whatUpdate"
                    app:enabled="true"
                    app:key="whatsUpdate"
                    android:key="@string/whatUpdate"/>

                <Preference
                    app:iconSpaceReserved="false"
                    android:summary="@string/author"
                    android:title="@string/verApp"
                    app:enabled="false"
                    android:key="verPreference"/>
        </PreferenceCategory>
</PreferenceScreen>

